    <version title="Dialogue with Trypho" author="Justin Martyr" fragment="4" language="Greek">
        <divisions>
            <division label="Chapter" Delimiter="."/>
            <division label="Paragraph"/>
        </divisions>
        <manuscripts>
            <ms abbrev="Goodspeed" language="Greek" show="yes">
